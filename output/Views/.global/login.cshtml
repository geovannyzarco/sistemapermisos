@using runnerDotNet;
@{
 XTempl Model = this.Model;
 if(ViewBag != null && ViewBag.xt != null)
 {
  Model = ViewBag.xt;
  if(Model != null)
  {
   Model.currentViewContext = ViewContext;
  }
 }
 string webRootPath = MVCFunctions.GetWebRootPath();
}

<!doctype html>@(new HtmlString(MVCFunctions.GetBuferContentAndClearBufer()))
<HTML @(Model.displayVar("html_attrs"))>
  <head>
    <META http-equiv="X-UA-Compatible" content="IE=Edge" />
    <META name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>
      @(Model.displayVar("pagetitle"))
    </title>
    <META http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <LINK REL="stylesheet" href="@(webRootPath)styles/default.css" type="text/css" />
    @foreach (string _rtlCSS in Model.GetSection("rtlCSS", this)){
      <LINK REL="stylesheet" href="@(webRootPath)styles/defaultRTL.css" type="text/css" />
    }
    @foreach (string _styleCSSFiles in Model.GetSection("styleCSSFiles", this)){
      <LINK REL="stylesheet" href="@(webRootPath)@(Model.displayVar("stylepath"))" type="text/css" />
    }
  </head>
  <BODY class="@(Model.displayVar("stylename")) function-login" @(Model.displayVar("bodyattrs"))>
    @foreach (string _body in Model.GetSection("body", this)){
      {Html.RenderSharedView("header");}
      <style>@MvcHtmlString.Create(@"")@(Model.displayVar("containerCss"))@MvcHtmlString.Create(@"")</style>
      <DIV class="r-panel-page">
        @foreach (string _supertop_block in Model.GetSection("supertop_block", this)){
          <DIV class="r-form" data-location="supertop" id="form_supertop_@(Model.displayVar("pageid"))" data-makeup="panel" @(Model.displayVar("form_supertop")) data-pageid="@(Model.displayVar("pageid"))">
            <DIV class="row">
              <DIV data-cellId="supertop_c1" data-pageid="@(Model.displayVar("pageid"))" @(Model.displayVar("cell_supertop_c1")) class="r-align-center r-ori-vert col-md-12">
              </div>
            </div>
          </div>
        }
        <DIV class="bs-pagepanel panel panel-primary">
          <DIV class="panel-heading">
            @foreach (string _top_block in Model.GetSection("top_block", this)){
              <DIV class="panel-title r-panel-top row" data-location="top" id="form_top_@(Model.displayVar("pageid"))" data-makeup="panel" @(Model.displayVar("form_top")) data-pageid="@(Model.displayVar("pageid"))">
                <DIV data-cellId="top_c1" data-pageid="@(Model.displayVar("pageid"))" @(Model.displayVar("cell_top_c1")) class="r-ori-vert col-md-6">
                  <H3 class="panel-title" data-itemtype="login_header" data-itemid="login_header" @(Model.displayVar("item_login_header")) data-pageid="@(Model.displayVar("pageid"))">
                    @(Model.displayVar("pagetitlelabel login"))
                  </h3>
                </div>
                <DIV data-cellId="top_c2" data-pageid="@(Model.displayVar("pageid"))" @(Model.displayVar("cell_top_c2")) class="r-align-right r-ori-vert col-md-6">
                </div>
              </div>
            }
          </div>
          <DIV class="panel-body">
            @foreach (string _above_grid_block in Model.GetSection("above-grid_block", this)){
              <DIV class="r-form" data-location="above-grid" id="form_above-grid_@(Model.displayVar("pageid"))" data-makeup="panel" @(Model.displayVar("form_above-grid")) data-pageid="@(Model.displayVar("pageid"))">
                <DIV class="row">
                  <DIV data-cellId="above-grid_c1" data-pageid="@(Model.displayVar("pageid"))" @(Model.displayVar("cell_above-grid_c1")) class="r-ori-vert col-md-12">
                    @foreach (string _firstAboveGridCell in Model.GetSection("firstAboveGridCell", this)){
                      foreach (string _message_block in Model.GetSection("message_block", this)){
                        <DIV data-itemtype="login_message" data-itemid="login_message" @(Model.displayVar("item_login_message")) data-pageid="@(Model.displayVar("pageid"))" class="alert @(Model.displayVar("message_class"))" role="alert">
                          @(Model.displayVar("message"))
                        </div>
                      }
                    }
                  </div>
                </div>
              </div>
            }
            @foreach (string _grid_block in Model.GetSection("grid_block", this)){
              <TABLE class="r-edit-form r-panel-form" cellpadding="0" cellspacing="0" data-location="grid" id="form_grid_@(Model.displayVar("pageid"))" data-makeup="panel" @(Model.displayVar("form_grid")) data-pageid="@(Model.displayVar("pageid"))" @(Model.displayVar("grid_hiddenattr")) data-form-width>
                <tbody>
                  <tr>
                    <TD class="r-align-right r-ori-vert col-md-4" data-single data-cellId="grid_c1" data-pageid="@(Model.displayVar("pageid"))">
                      <DIV edit-form-cell class="clearfix">
                        <LABEL data-itemtype="username_label" data-itemid="username_label" @(Model.displayVar("item_username_label")) data-pageid="@(Model.displayVar("pageid"))" data-small class="r-edit-label control-label" for="username">
                          @foreach (string _username_label in Model.GetSection("username_label", this)){<text>
                            Nombre de usuario:
                          </text>}
                        </label>
                      </div>
                    </td>
                    <TD class="r-ori-vert col-md-6" data-single data-cellId="grid_c2" data-pageid="@(Model.displayVar("pageid"))">
                      <DIV edit-form-cell class="clearfix">
                        <DIV class="r-edit-field" data-itemtype="username" data-itemid="username" @(Model.displayVar("item_username")) data-pageid="@(Model.displayVar("pageid"))">
                          <INPUT type="text" class="form-control" name="username" @(Model.displayVar("username_attrs")) id="username" />
                        </div>
                      </div>
                    </td>
                    <TD class="col-md-2">
                    </td>
                  </tr>
                  <tr>
                    <TD class="r-align-right r-ori-vert col-md-4" data-single data-cellId="grid_c3" data-pageid="@(Model.displayVar("pageid"))">
                      <DIV edit-form-cell class="clearfix">
                        <LABEL data-itemtype="password_label" data-itemid="password_label" @(Model.displayVar("item_password_label")) data-pageid="@(Model.displayVar("pageid"))" data-small class="r-edit-label control-label" for="password">
                          @foreach (string _password_label in Model.GetSection("password_label", this)){<text>
                            Contraseña:
                          </text>}
                        </label>
                      </div>
                    </td>
                    <TD class="r-ori-vert col-md-6" data-single data-cellId="grid_c4" data-pageid="@(Model.displayVar("pageid"))">
                      <DIV edit-form-cell class="clearfix">
                        <DIV class="r-edit-field" data-itemtype="password" data-itemid="password" @(Model.displayVar("item_password")) data-pageid="@(Model.displayVar("pageid"))">
                          <INPUT type="password" class="form-control" name="password" @(Model.displayVar("password_attrs")) id="password" />
                        </div>
                      </div>
                    </td>
                    <TD class="col-md-2">
                    </td>
                  </tr>
                  <tr>
                    <TD class="r-ori-vert col-md-4" data-single data-cellId="grid_c9" data-pageid="@(Model.displayVar("pageid"))">
                      <DIV edit-form-cell class="clearfix">
                      </div>
                    </td>
                    <TD class="r-ori-vert col-md-6" data-single data-cellId="grid_c10" data-pageid="@(Model.displayVar("pageid"))">
                      <DIV edit-form-cell class="clearfix">
                        <DIV class="checkbox r-edit-field" data-itemtype="remember_password" data-itemid="remember_password" @(Model.displayVar("item_remember_password")) data-pageid="@(Model.displayVar("pageid"))">
                          <label>
                            <INPUT type="checkbox" @(Model.displayVar("rememberbox_attrs")) />
                            @foreach (string _remember_password_label in Model.GetSection("remember_password_label", this)){
                              <span>
                                Recordarme
                              </span>
                            }
                          </label>
                        </div>
                      </div>
                    </td>
                    <TD class="col-md-2">
                    </td>
                  </tr>
                </tbody>
              </table>
            }
            @foreach (string _footer_block in Model.GetSection("footer_block", this)){
              <DIV class="r-form" data-location="footer" id="form_footer_@(Model.displayVar("pageid"))" data-makeup="panel" @(Model.displayVar("form_footer")) data-pageid="@(Model.displayVar("pageid"))">
                <DIV class="row">
                  <DIV data-cellId="footer_c1" data-pageid="@(Model.displayVar("pageid"))" data-page=".global_login" @(Model.displayVar("cell_footer_c1")) class="r-ori-vert col-md-6">
                  </div>
                  <DIV data-cellId="footer_c2" data-pageid="@(Model.displayVar("pageid"))" data-page=".global_login" @(Model.displayVar("cell_footer_c2")) class="col-md-6">
                    @foreach (string _signin_button in Model.GetSection("signin_button", this)){
                      <SPAN data-itemtype="login_button" data-itemid="login_button" @(Model.displayVar("item_login_button")) data-pageid="@(Model.displayVar("pageid"))" data-small data-button-item>
                        <A type="button" title class="btn btn-primary" @(Model.displayVar("loginlink_attrs"))>
                          Aceptar
                        </a>
                      </span>
                    }
                    @foreach (string _continue_button in Model.GetSection("continue_button", this)){
                      <SPAN data-itemtype="continue_login_button" data-itemid="continue_login_button" @(Model.displayVar("item_continue_login_button")) data-pageid="@(Model.displayVar("pageid"))" data-small data-button-item>
                        <A type="button" title class="btn btn-primary" @(Model.displayVar("continuebutton_attrs"))>
                          Continuar
                        </a>
                      </span>
                    }
                    @foreach (string _guestlink_block in Model.GetSection("guestlink_block", this)){
                      <SPAN data-itemtype="guest_login" data-itemid="guest_login" @(Model.displayVar("item_guest_login")) data-pageid="@(Model.displayVar("pageid"))" data-small data-button-item>
                        <A type="button" title class="btn btn-link" @(Model.displayVar("guestlink_attrs"))>
                          Conectar como invitado
                        </a>
                      </span>
                    }
                  </div>
                </div>
              </div>
            }
          </div>
        </div>
        @foreach (string _superbottom_block in Model.GetSection("superbottom_block", this)){
          <DIV class="r-form" data-location="superbottom" id="form_superbottom_@(Model.displayVar("pageid"))" data-makeup="panel" @(Model.displayVar("form_superbottom")) data-pageid="@(Model.displayVar("pageid"))">
          </div>
        }
      </div>
      {Html.RenderSharedView("footer");}
    }
  </body>
</html>
